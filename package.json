{
  "name": "wolsey-associates-crm",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn --cwd packages/server dev\" \"yarn --cwd apps/mobile start\"",
    "bootstrap": "yarn install",
    "lint": "echo \"Add lint scripts\"",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "@babel/core": "^7.20.0"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "react-dom": "18.2.0",
    "react-native": "0.71.14",
    "react-native-web": "~0.18.10",
    "expo": "~48.0.21",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0"
  }
}
